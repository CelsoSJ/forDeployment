{
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python",
      "config": {
        "outputDirectory": "staticfiles"
      }
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "/manage.py" }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "sorsu_bc_dms_web_app.settings.production"
  }
}
