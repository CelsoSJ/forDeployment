{
  "build": {
    "env": {
      "DJANGO_SETTINGS_MODULE":"sorsu_bc_dms_web_app.settings.production"
    },
    "cmd": "pip install -r requirements.txt && python manage.py collectstatic --noinput"
  },
  "routes": [
    {"src": "/static(.*)", "dest": "/static/$1"},
    {"src": "/(.*)", "dest": "Login.html"}
  ]
}